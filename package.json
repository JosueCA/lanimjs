{
  "name": "lanimjs",
  "version": "0.1.13",
  "description": "Application made with web technologies that runs integrated terminals",
  "main": "index.js",
  "engines": {
    "node": ">=22"
  },
  "scripts": {
    "electron-builder": "electron-builder build --linux AppImage"
  },
  "author": "",
  "license": "ISC",
  "build": {
    "appId": "lanimjs",
    "nodeGypRebuild": false,
    "copyright": "Copyright Â© 2024 JosueCA",
    "icon": "libs/icons/appIcon_flaticon.png",
    "files": [
      "index.js",
      "index.html",
      "preload.js",
      "web.js",
      "web-styles.js",
      "libs",
      {
        "from": "./*",
        "to": "./*",
        "filter": [
          "**/*"
        ]
      }
    ],
    "linux": {
      "category": "Terminal"
    }
  },
  "devDependencies": {
    "electron": "24.1.2",
    "electron-builder": "23.6.0"
  },
  "dependencies": {
    "ajv": "8.12.0",
    "better-sqlite3": "11.7.0",
    "node-pty": "0.10.1",
    "os-utils": "0.0.14"
  }
}
